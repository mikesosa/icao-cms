    "modules": [
      {
        "title": "Paper 1 - Listening Comprehension",
        "moduleType": "core",
        "description": "Develop skills to understand standard and non-standard ATC communications.",
        "units": [
          {
            "title": "Part 1 - Routine Communications",
            "contentBlocks": [
              {
                "type": "text",
                "content": "Understanding standard ATC phraseology and clearances",
                "order": 1
              },
              {
                "type": "video",
                "content": "https://cdn.example.com/videos/routine-atc-communications.mp4",
                "order": 2
              }
            ],
            "practice": {
              "title": "Practice - Routine Communications",
              "description": "Practice understanding standard ATC instructions",
              "activities": [
                {
                  "type": "mcq",
                  "prompt": "What is the assigned heading?",
                  "media": {
                    "url": "https://cdn.example.com/audio/heading-assignment.mp3"
                  },
                  "options": ["Heading 270", "Heading 090", "Heading 180"],
                  "correctAnswer": "Heading 270",
                  "feedback": "Listen for the heading digits and direction."
                },
                {
                  "type": "mcq",
                  "prompt": "What is the clearance limit?",
                  "media": {
                    "url": "https://cdn.example.com/audio/clearance-limit.mp3"
                  },
                  "options": ["FL350", "FL250", "FL150"],
                  "correctAnswer": "FL350",
                  "feedback": "Pay attention to the flight level numbers."
                }
              ]
            }
          },
          {
            "title": "Part 2 - Emergency Communications",
            "contentBlocks": [
              {
                "type": "text",
                "content": "Understanding emergency and non-standard communications",
                "order": 1
              },
              {
                "type": "video",
                "content": "https://cdn.example.com/videos/emergency-communications.mp4",
                "order": 2
              }
            ],
            "practice": {
              "title": "Emergency Communications Practice",
              "description": "Practice understanding emergency situations",
              "activities": [
                {
                  "type": "mcq",
                  "prompt": "What is the nature of the emergency?",
                  "media": {
                    "url": "https://cdn.example.com/audio/emergency-report.mp3"
                  },
                  "options": [
                    "Engine failure",
                    "Medical emergency",
                    "Fuel shortage"
                  ],
                  "correctAnswer": "Engine failure",
                  "feedback": "Listen for specific emergency terminology."
                }
              ]
            }
          }
        ]
      },
      {
        "title": "Paper 2 - Speaking Skills",
        "moduleType": "oral",
        "description": "Develop clear and effective speaking skills for ATC communications.",
        "units": [
          {
            "title": "Task 1 - Routine Communications",
            "contentBlocks": [
