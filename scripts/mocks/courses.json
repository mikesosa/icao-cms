              },
              {
                "type": "video",
                "content": "https://cdn.example.com/videos/standard-phraseology.mp4",
                "order": 2
              }
            ],
            "practice": {
              "title": "Routine Communications Practice",
              "description": "Practice giving standard ATC instructions",
              "activities": [
                {
                  "type": "audio_input",
                  "prompt": "Issue a departure clearance",
                  "media": null,
                  "feedback": "Include all necessary elements: call sign, clearance limit, route, and initial altitude."
                }
              ]
            }
          },
          {
            "title": "Task 2 - Emergency Communications",
            "contentBlocks": [
              {
                "type": "text",
                "content": "Practice handling emergency situations",
                "order": 1
              },
              {
                "type": "video",
                "content": "https://cdn.example.com/videos/emergency-handling.mp4",
                "order": 2
              }
            ],
            "practice": {
              "title": "Emergency Communications Practice",
              "description": "Practice handling emergency situations",
              "activities": [
                {
                  "type": "audio_input",
                  "prompt": "Handle an aircraft reporting engine failure",
                  "media": null,
                  "feedback": "Provide clear instructions, prioritize safety, and coordinate emergency services."
                }
              ]
            }
          }
        ]
